----SQL implement
--drop all the table
drop table stockmarket;
drop table dealer;
drop table customer;
drop table analyst;
drop table company; 
drop table focus;
drop table belong;
drop table placeorder;


--create table, stockmarket
CREATE TABLE stockmarket(
symbol VARCHAR(30),
stockname VARCHAR(60),
volume INT, --BIGINT
price REAL,
lastprice REAL,
fiftyday_avg REAL,
marketcap INT,
thirtymon_avg INT,
PRIMARY KEY(symbol)
);

--create table, dealer
CREATE TABLE dealer(
workerid INT,
agentname VARCHAR(40),
address VARCHAR(40),
phone INT,
PRIMARY KEY(workerid,agentname)
);

--create table, customer
CREATE TABLE customer(
customername VARCHAR(40),
accountid INT,
address VARCHAR(40)
DEFAULT 'unknown',
PRIMARY KEY(accountid)
);

--create table, analyst
CREATE TABLE analyst(
analystname VARCHAR(40),
expertise VARCHAR(40),
PRIMARY KEY(analystname,expertise)
);

--create table, company
CREATE TABLE company(
companyname VARCHAR(40),
address VARCHAR(40),
PRIMARY KEY(companyname)
);

--create table, focus
CREATE TABLE focus(
analystname VARCHAR(40),
stockname VARCHAR(60),
symbol VARCHAR(40),
PRIMARY KEY(analystname,symbol)
);

--create table, belong
CREATE TABLE belong(
analystname VARCHAR(40),
companyname VARCHAR(40),
PRIMARY KEY(analystname,companyname)
);

--create table, placeorder
CREATE TABLE placeorder(
symbol VARCHAR(40),
stockname VARCHAR(60),
workerid INT,
accountid INT
);

